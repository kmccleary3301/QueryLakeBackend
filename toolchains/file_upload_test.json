{
	"name": "File Upload Test",
    "id": "test_file_upload",
    "category": "Test",
    "initial_state": {
        "title": "Chat",
        "chat_history": []
    },
    "nodes": [
        {
            "id": "user_file_upload_event",
            "is_event": true,
            "input_arguments": [
                {
                    "key": "auth",
                    "from_user": true
                },
                {
                    "key": "<<FILE>>",
                    "from_user": true
                }
            ],
            "feed_mappings": [
                {
                    "destination": "<<FILES>>",
                    "sequence": [
                        {
                            "type": "createAction",
                            "route" : [ "single_upload" ]
                        }
                    ],
                    "getFromInputs": { "route": [ "<<FILE>>" ] }
                },
                {
                    "destination": "ocr",
                    "sequence": [
                        {
                            "type": "createAction",
                            "route" : [ "file" ]
                        }
                    ],
                    "getFromInputs": { "route": [ "<<FILE>>" ] }
                }
            ]
        },
        {
            "id": "ocr",
            "api_function": "ocr_pdf_file",
            "input_arguments": [
                {
                    "key": "database",
                    "from_server": true
                },
                {
                    "key": "auth",
                    "from_user": true
                },
                {
                    "key": "file",
                    "from_files": {
                        "route": [ "single_upload" ]
                    }
                }
            ],
            "feed_mappings": [
                {
                    "destination": "<<USER>>",
                    "sequence": [
                        {
                            "type": "createAction",
                            "route" : [ "output" ]
                        }
                    ],
                    "getFromOutputs": {
                        "route" : [ "pdf_text" ]
                    }
                },
                {
                    "destination": "<<STATE>>",
                    "sequence": [
                        {
                            "type": "createAction",
                            "route" : [ "output" ]
                        }
                    ],
                    "getFromOutputs": {
                        "route": [ "pdf_text" ]
                    }
                }
            ]
        }
    ]
}