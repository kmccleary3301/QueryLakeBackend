{
	"name": "Iteration Test",
    "id": "iterable_test",
    "category": "Test",
    "initial_state": {
        "title": "Chat",
        "chat_history": [],
        "question_list_iterated": [],
        "iterable_output": []
    },
    "nodes": [
        {
            "id": "user_question_event",
            "is_event": true,
            "input_arguments": [
                {
                    "key": "model_parameters",
                    "from_user": true,
                    "optional": true
                },
                {
                    "key": "auth",
                    "from_user": true
                },
                {
                    "key": "collection_hash_ids",
                    "from_user": true,
                    "optional": true
                },
                {
                    "key": "question_list",
                    "initialValue": [
                        "Who is Magic Johnson?",
                        "Who is Michael Jordan?",
                        "Who is Kobe Bryant?",
                        "Who is Shaquille O'neal?",
                        "Who is Lebron James?",
                        "Who is Steph Curry?",
                        "Who is Kevin Durant?"
                    ]
                }
            ],
            "feed_mappings": [
                {
                    "destination": "third_step",
                    "sequence": [
                        {
                            "type": "createAction",
                            "route" : [ "model_parameters" ]
                        }
                    ],
                    "getFromInputs": { "route": [ "model_parameters" ] }
                },
                {
                    "destination": "third_step",
                    "sequence": [
                        {
                            "type": "createAction",
                            "route" : [ "auth" ]
                        }
                    ],
                    "getFromInputs": { "route": [ "auth" ] }
                },
                {
                    "destination": "third_step",
                    "sequence": [
                        {
                            "type": "createAction", 
                            "initialValue": {
                                "type": "staticValue",
                                "value": [{
                                    "role": "user"           
                                }]
                            },
                            "insertion_values": [ null ],
                            "insertions": [ [ 0, "content" ] ],
                            "route" : [ "chat_history" ]
                        }
                    ],
                    "getFromInputs": {
                        "route": [ "question_list" ]
                    },
                    "iterate": true
                }
            ]
        },
        {
            "id": "third_step",
            "api_function": "llm",
            "input_arguments": [
                {
                    "key": "chat_history"
                },
                {
                    "key": "auth"
                },
                {
                    "key": "model_parameters",
                    "optional": true
                }
            ],
            "feed_mappings": [
                {
                    "destination": "<<USER>>",
                    "getFromOutputs": {
                        "route" : [ "output" ]
                    }
                },
                {
                    "destination": "<<STATE>>",
                    "sequence": [
                        {
                            "type": "appendAction", 
                            "initialValue": {
                                "type": "staticValue",
                                "value": {
                                    "role": "assistant"           
                                }
                            },
                            "insertion_values": [ null ],
                            "insertions": [ [ "content" ] ],
                            "route" : [ "iterable_output" ]
                        }
                    ],
                    "getFromOutputs": {
                        "route": [ "output" ]
                    },
                    "stream": true,
                    "stream_initial_value": ""
                }
            ]
        }
    ]
}